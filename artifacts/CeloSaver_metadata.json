					"details": "Deposit funds into a specific savings goal",
					"params": {
						"_goalId": "Index of the goal to deposit into"
					}
				},
				"getGoal(address,uint256)": {
					"details": "Get a specific goal for a user",
					"params": {
						"_goalId": "Index of the goal",
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "The savings goal"
					}
				},
				"getGoalCount(address)": {
					"details": "Get the number of goals for a user",
					"params": {
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "Number of goals"
					}
				},
				"getGoalProgress(address,uint256)": {
					"details": "Get progress percentage for a goal",
					"params": {
						"_goalId": "Index of the goal",
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "Progress percentage (0-100)"
					}
				},
				"getTotalSavings(address)": {
					"details": "Get total savings for a user across all goals",
					"params": {
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "Total savings amount"
					}
				},
				"getUserGoals(address)": {
					"details": "Get all goals for a user",
					"params": {
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "Array of savings goals"
					}
				},
				"isGoalCompleted(address,uint256)": {
					"details": "Check if a goal has reached its target",
					"params": {
						"_goalId": "Index of the goal",
						"_user": "Address of the user"
					},
					"returns": {
						"_0": "True if goal is completed"
					}
				},
				"withdraw(uint256,uint256)": {
					"details": "Withdraw funds from a specific savings goal",
					"params": {
						"_amount": "Amount to withdraw (in wei)",
						"_goalId": "Index of the goal to withdraw from"
					}
